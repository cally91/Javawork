package com.biz.myclass;

public class PrimeClass {

	 public void prime(int lastNum) {
		 for (int i=1; i<=lastNum;i++) {
			 int index=0;
			 for(index=2; index<=i; index++) {
				 if (i%index==0) {
					 break;
				 }
			 }
			 if(index>=i) {
				 System.out.println(i+"는 소수입니다.");
			 }
		 }
	 
	 }

	 public void prime() {
		 for (int i=1; i<=100;i++) {
			 int index=0;
			 for(index=2; index<=i; index++) {
				 if (i%index==0) {
					 break;
				 }
			 }
			 if(index>=i) {
				 System.out.println(i+"는 소수입니다.");
			 }
		 }
	 
	 }
}
